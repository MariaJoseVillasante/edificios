<div id="<%= dom_id blog %>">
  <p>
    <strong>Title:</strong>
    <%= blog.title %>
  </p>

  <p>
    <strong>Image url:</strong>
    <%= blog.image_url %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= blog.content %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= blog.author %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= blog.status %>
  </p>

</div>
